<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <daniilloban_config>
            <general>
                <enabled>1</enabled>
                <is_show_qty>1</is_show_qty>
                <default_qty>0</default_qty>
            </general>
            <cron_group>
                <email_template>amasty_daniilloban_blacklist_template</email_template>
                <email>user@amasty.com</email>
                <frequency>* * * * *</frequency>
            </cron_group>
        </daniilloban_config>
    </default>
</config>